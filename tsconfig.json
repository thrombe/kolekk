{
    "extends": "./.svelte-kit/tsconfig.json",
    "compilerOptions": {
        "allowJs": true,
        "checkJs": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "resolveJsonModule": true,
        "skipLibCheck": true,
        "sourceMap": true,
        "typeRoots": [
            "node_modules/@types"
        ],
        "paths": {
            "$lib/*": [
                "./src/lib/*"
            ],
            "types": [
                "./src/rs_bindings.ts"
            ]
        },
        "allowImportingTsExtensions": true,
        "types": [
            "youtube"
        ],
        "strict": true
    }
}