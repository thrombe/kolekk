{
    "name": "kolekk",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "tauri-dev": "cargo build --no-default-features && mv ./target/debug/src-tauri ./target/debug/kolekk && target/debug/kolekk",
        "tauri-dev-build": "cargo build --no-default-features && mv ./target/debug/src-tauri ./target/debug/kolekk",
        "tauri-build-windows": "cargo tauri build --target x86_64-pc-windows-msvc",
        "dev": "vite dev",
        "build": "vite build",
        "preview": "vite preview",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "lint": "prettier --plugin-search-dir . --check . && eslint .",
        "format": "prettier --plugin-search-dir . --write ."
    },
    "devDependencies": {
        "@sveltejs/vite-plugin-svelte": "^2.4.5",
        "@typescript-eslint/eslint-plugin": "^5.45.0",
        "@typescript-eslint/parser": "^5.45.0",
        "eslint": "^8.28.0",
        "eslint-config-prettier": "^8.5.0",
        "prettier": "^2.8.0",
        "prettier-plugin-svelte": "^2.8.1",
        "svelte": "^4.2.0",
        "svelte-check": "^3.4.3",
        "svelte-preprocess": "^5.0.4",
        "tslib": "^2.4.1",
        "typescript": "^5.0.0",
        "vite": "^4.4.2"
    },
    "type": "module",
    "dependencies": {
        "@tauri-apps/api": "^1.2.0",
        "@tauri-apps/cli": "^1.2.3",
        "@types/youtube": "^0.0.47",
        "svelte-spa-router": "^3.3.0",
        "youtubei.js": "^6.0.0"
    }
}
